#notebook
  ##.format格式化
  	 #first version：
		a = "浙江"
	 	b = "嘉兴"
	  	print("{},{}".format(a,b))
	  	#结果显示：浙江,嘉兴
	 #second version:
	 	print ("{:2f}".format(321.123))
		print("{:6f}and{:5f}".format(321.123,123.321))
		print("{:2f}".format(10))
		print("{:50f}".format(10))
		#结果显示：321.123000
				  321.123000and123.321000
				  10.000000
				                                          10.000000

